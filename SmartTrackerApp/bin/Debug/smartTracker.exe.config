<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="Desc_serverPort" value="Integer value for server TCP port - default : 6901"/>
    <add key="serverPort" value="6901"/>
    <add key="Desc_NbRecordToKeep" value="Integer of amount of full scan stored"/>
    <add key="NbRecordToKeep" value="150"/>
    <add key="Desc_bStoreEventTag" value="Boolean to enable of storing tag history"/>
    <add key="bStoreEventTag" value="True"/>
    <add key="Desc_TimeZoneOffset" value="Double value to set the offset hour from GMT"/>
    <add key="TimeZoneOffset" value="1.0"/>
    <add key="Desc_bShowServer" value="Boolean to enable server mode at startup"/>
    <add key="bShowServer" value="False"/>
    <add key="Desc_bCompareOnLotID" value="Boolean to enable the server window at startup"/>
    <add key="bCompareOnLotID" value="True"/>
    <add key="Desc_bScreenSaver" value="Boolean to enable the screen saver"/>
    <add key="bScreenSaver" value="True"/>
    <add key="Desc_bShowImage" value="Boolean to enable the image association for each item"/>
    <add key="bShowImage" value="True"/>
    <add key="Desc_bShowGroupMode" value="Boolean to enable the live Data with the group mode feature"/>
    <add key="bShowGroupMode" value="False"/>
    <add key="Desc_bShowAlert" value="Boolean to show a message box for each alert in case of no smtp"/>
    <add key="bShowAlert" value="True"/>
    <add key="Desc_bUseAlarm" value="Boolean to enable the alarm feature"/>
    <add key="bUseAlarm" value="True"/>
    <add key="Desc_timeTcpToRefresh" value="Integer in ms to refresh the TCP device"/>
    <add key="timeTcpToRefresh" value="5000"/>
    <add key="Desc_pathExcelSheet" value="Full path to the xlsm document containing the macro"/>
    <add key="pathExcelSheet" value="C:\Temp\TestMacro\facture.xlsm"/>
    <add key="Desc_MacroName" value="Name of the macro contain in the document excel"/>
    <add key="MacroName" value="Test"/>
    <add key="Desc_bViewerMode" value="Enable Disable Viewer Mode - To display the inventory from a shared database with no action on reader"/>
    <add key="bViewerMode" value="False"/>
    <add key="Desc_bAutoUpdate" value="Enable Disable Auto Update Mode - With True the update will be checked at each startup"/>
    <add key="bAutoUpdate" value="False"/>
    <add key="Desc_bInterruptScanWithFP" value="If true, user stop the current scan if well authentified on the fingerprint"/>
    <add key="bInterruptScanWithFP" value="False"/>
    <add key="ClientSettingsProvider.ServiceUri" value=""/>
    <add key="bUseCsvExchange" value="False"/>
    <add key="Desc_bUseCsvExchange" value="If true use CSV file exchange data"/>
    <add key="pathCsvExchange" value="C:\Users\Public\TestEurostar"/>
    <add key="Desc_pathCsvExchange" value="Path of shared folder to CSV  exchange"/>
    <add key="CsvMachineId" value="0007"/>
    <add key="Desc_CsvMachineId" value="Machine ID (4 digits)"/>
    <add key="TcpUnlockingAuthorization" value="Put Unlocking String"/>
    <add key="Desc_TcpUnlockingAuthorization" value="Put Authorizing string to unlock door with TCP command"/>
  </appSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri=""/>
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400"/>
      </providers>
    </roleManager>
  </system.web>
<startup><supportedRuntime version="v2.0.50727"/></startup></configuration>
